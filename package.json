  "name": "react-flowfield",
  "author": "bjoroen",
  "repository": {
    "type": "git",
    "url": "https://github.com/bjoroen/react-flowfield"
  },
  "keywords": [
    "react background",
    "background component",
    "flowfield",
    "perlin noise"
  ],
  "module": "dist/crative-react-components-lib.esm.js",
